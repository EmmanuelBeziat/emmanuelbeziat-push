{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Dev",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "dev"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Prod",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "prod"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "deploy"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "test"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Route Tests",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "test:routes"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Environment Tests",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "args": ["run", "test:env"],
      "console": "integratedTerminal"
    }
  ]
}
